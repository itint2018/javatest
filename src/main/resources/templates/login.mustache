{{>header}}
<div class="container">
    <form action="/api/login" method="POST" onsubmit="OnSubmit(this);return false">
        <div class="mb-3">
            <label for="login" class="form-label">Имя пользователя</label>
            <input type="text" class="form-control" id="login" name="login" autofocus><br>
            <label for="password" class="form-label">Пароль</label>
            <input type="password" class="form-control" id="pass"
                   name="pass"><br>
            <input type="submit" class="btn btn-secondary" value="Войти" name="submit">
        </div>
    </form>
</div>
<script src="/../js/app.js"></script>
<script src="/../js/md5_min.js"></script>
{{>footer}}